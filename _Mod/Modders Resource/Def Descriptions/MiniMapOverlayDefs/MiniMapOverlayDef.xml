        <updateInterval>int</updateInterval>
                                        [required] Update frequency of overlay (ticks).  Default: 250

        <drawOffset>int</drawOffset>    [optional] Index order to draw overlay in minimap.  Default: 0
        
        <hiddenByDefault>bool</hiddenByDefault>
                                        [optional] Hide overlay by default.  Default: false
        
        <miniMapDef>MiniMapDef</miniMapDef>
                                        [optional] MiniMapDef to add this overlay to (can be defined here or in MiniMapDef)

    </MiniMapOverlayDef>

